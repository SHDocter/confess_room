{
    "version": 2,
    "builds": [
        {
            "src": "*.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/(.*)",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/javascript"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}